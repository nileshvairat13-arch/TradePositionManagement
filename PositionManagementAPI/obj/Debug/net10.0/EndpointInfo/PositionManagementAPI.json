{
  "openapi": "3.0.4",
  "info": {
    "title": "PositionManagementAPI",
    "version": "1.0"
  },
  "paths": {
    "/api/Transactions": {
      "post": {
        "tags": [
          "Transactions"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Transaction"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Transaction"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Transaction"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Transactions/positions": {
      "get": {
        "tags": [
          "Transactions"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Transaction": {
        "required": [
          "action",
          "buySell",
          "securityCode",
          "tradeId",
          "version"
        ],
        "type": "object",
        "properties": {
          "transactionId": {
            "type": "integer",
            "format": "int32"
          },
          "tradeId": {
            "type": "integer",
            "format": "int32"
          },
          "version": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "securityCode": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string"
          },
          "quantity": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "action": {
            "minLength": 1,
            "pattern": "INSERT|UPDATE|CANCEL",
            "type": "string"
          },
          "buySell": {
            "minLength": 1,
            "pattern": "Buy|Sell",
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  },
  "tags": [
    {
      "name": "Transactions"
    }
  ]
}